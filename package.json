    "electron": "13.1.6",
    "electron-builder": "22.11.7",
    "electron-reloader": "1.2.1",
    "elm": "0.19.1-5",
    "elm-live": "4.0.2",
    "live-server": "1.2.1",
    "node-elm-compiler": "5.0.6",
    "parcel": "2.0.0-beta.2",
    "typescript": "4.3.5",
